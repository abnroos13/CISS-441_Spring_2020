# Declarations
my_name = 'Chris'
age = 29
state = 'KY'
height = 72

# use vars in print statement
print(' Hello world')
print(' - my name is ' + my_name)
print(' - my age is ' + str(age))
print(' - I live in {0}.' .format(state))
print(' - I am ' + str(height) + ' tall')
